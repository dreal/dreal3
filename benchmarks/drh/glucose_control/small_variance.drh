{
    // inv1 > 0 and inv2 <= 0
    mode 5;

    invt:
    ((Gp/Vgs) >= aclow);
    ((Gp/Vgs) <= achigh);
    (inv1 > 0);
    (inv2 <= 0);
    (tau <= prd - perr);

    flow:
    //state variables
    d/dt[Ip]= -((m2s+m4s)*Ip) + (m1s*Il) + u/BWs;
    d/dt[X]=  (P2us*Ip/Vis) - (P2us*X) - (P2us*Ibs);
    d/dt[I1]= (kis*Ip/Vis) - (kis*I1);
