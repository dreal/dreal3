                            (kp1' = kp1)
                            (kp2' = kp2)
                            (kp3' = kp3)
                            (Vm0' = Vm0)
                            (Vmx' = Vmx)
                            (Km0' = Km0)
                            (Vg' = Vg)
                            (u' = u)
                            (m' = m)
                            (tau' = tau)
                            (simtime' = simtime));

        (and (tau >= (prd-perr)) (inv1 > 0) (inv2 > 0)) ==> @10 (and (Ip' = Ip)
                            (X' = X)
                            (I1' = I1)
                            (Id' = Id)
                            (Il' = Il)
