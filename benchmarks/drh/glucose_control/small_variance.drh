                            (ki' = ki)
                            (P2u' = P2u)
                            (Vi' = Vi)
                            (Ib' = Ib)
                            (BW' = BW)
                            (k1' = k1)
                            (k2' = k2)
                            (kp1' = kp1)
                            (kp2' = kp2)
                            (kp3' = kp3)
                            (Vm0' = Vm0)
                            (Vmx' = Vmx)
                            (Km0' = Km0)
                            (Vg' = Vg)
                            (u' = 0)
                            (m' = 0)
                            (tau' = 0)
                            (simtime' = simtime));
