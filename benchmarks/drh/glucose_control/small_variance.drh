                            (BW' = BW)
                            (k1' = k1)
                            (k2' = k2)
                            (kp1' = kp1)
                            (kp2' = kp2)
                            (kp3' = kp3)
                            (Vm0' = Vm0)
                            (Vmx' = Vmx)
