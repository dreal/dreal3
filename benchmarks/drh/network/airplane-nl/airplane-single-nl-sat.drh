	d/dt[phi]     = p;
	d/dt[psi]     = (g / vT) * tan(phi);
        d/dt[xAIL]    = 0.25;
        d/dt[xRDR]    = 0.5;
	d/dt[gAIL]    = 0;
	d/dt[gRDR]    = 0;
	d/dt[gDir]    = 0;
