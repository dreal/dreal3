(set-logic QF_NRA_ODE)
(declare-fun x () Real)
(declare-fun x_0 () Real)
(declare-fun x_t () Real)
(declare-fun time_0 () Real)
(define-ode flow_0 ((= d/dt[x] 0)))
(assert (>= time_0 0))
(assert (<= time_0 0.01))
(assert (= x_0 0))
(assert (not (= x_t 0)))
(assert (= [x_t]
           (integral 0. time_0 [x_0] flow_0)))
(check-sat)
(exit)
