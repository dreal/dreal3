(set-logic QF_NRA)
(declare-fun x0 () Real)
(declare-fun x1 () Real)
(declare-fun x2 () Real)
(declare-fun x3 () Real)
(assert (<= x0 7.5))
(assert (<= 2.5 x0))
(assert (<= x1 7.5))
(assert (<= 2.5 x1))
(assert (<= x2 7.5))
(assert (<= 2.5 x2))
(assert (<= x3 7.5))
(assert (<= 2.5 x3))
(assert (and
         (or (<= 5.0 (- x2 x3)) (<= 5.0 (- x3 x2)))
         (or (<= 5.0 (- x0 x2)) (<= 5.0 (- x2 x0)))
         (or (<= 5.0 (- x1 x2)) (<= 5.0 (- x2 x1)))
         (or (<= 5.0 (- x1 x3)) (<= 5.0 (- x3 x1)))
         (or (<= 5.0 (- x0 x1)) (<= 5.0 (- x1 x0)))
         (or (<= 5.0 (- x0 x3)) (<= 5.0 (- x3 x0)))
         ))
(check-sat)
(exit)
