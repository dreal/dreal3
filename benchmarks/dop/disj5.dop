prec: 0.05

var:

[-10, 10] x1;
[-10, 10] x2;
[-10, 10] x3;
[-10, 10] x4;
[-10, 10] x5;
[-10, 10] x6;

cost:

(cos(sin(x1^2 - x2^2)) +
 cos(sin(x2^2 - x3^2)) +
 cos(sin(x3^2 - x4^2)) +
 cos(sin(x4^2 - x5^2)) +
 cos(sin(x5^2 - x6^2))) /
(1 + 0.00001 * (x1^2 + x2^2 + x3^2 + x4^2 + x5^2 + x6^2));

ctr:

x1^2 + x2^2 + x3^2 < 100 || (x2-5)^2 + (x3-5)^2 + (x4-5)^2 < 100;
x3^2 + x4^2 + x5^2 < 100 || (x4+5)^2 + (x5+5)^2 + (x6+5)^2 < 100;
