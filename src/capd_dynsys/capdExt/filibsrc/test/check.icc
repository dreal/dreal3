void checkPredSucc() 
{
  int n = predSuccTestSetSize[0];
  TestSet<double> testSet(n, predSuccTestSet);

  // pred()
  filib::pred_fo<double> pred_fo;
  allErrors += UnCheck<double, double>("pred", pred_fo,
				testSet,
				TestSet<double>(n, predExpected),
				Comparator<double>()
				);

  // succ()
