	template <typename N, rounding_strategy K, interval_mode E>
	bool interval<N,K,E>::proper_superset(interval<N,K,E> const & a) const
	{
		if ( E )
			return (INF <= a.INF && a.SUP <= SUP && (INF < a.INF || a.SUP < SUP)) ||
				(a.isEmpty() && ! isEmpty());
		else
			return (INF <= a.INF && a.SUP <= SUP && (INF < a.INF || a.SUP < SUP));
	}

