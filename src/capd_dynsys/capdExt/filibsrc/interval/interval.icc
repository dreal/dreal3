  template <typename N, rounding_strategy K, interval_mode E>
	std::ostream & interval<N,K,E>::bitImage(std::ostream & os) const
	{
		os << "[ ";
		primitive::basicBitImage(INF,os);
		os << " ," << "\n" << "  ";
		primitive::basicBitImage(SUP,os);
		os << " ]" << "\n";
		return os;
	}

